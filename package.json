{
  "name": "vanilla-jsx",
  "type": "module",
  "version": "0.0.0",
  "description": "Vanilla JSX is a library for building user interfaces with JavaScript/TypeScript and JSX/TSX syntax.",
  "author": "Drswith",
  "license": "MIT",
  "homepage": "https://github.com/Drswith/vanilla-jsx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drswith/vanilla-jsx.git"
  },
  "bugs": {
    "url": "https://github.com/Drswith/vanilla-jsx/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.mts",
      "import": "./dist/jsx-runtime.mjs",
      "require": "./dist/jsx-runtime.mjs"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.mts",
      "import": "./dist/jsx-dev-runtime.mjs",
      "require": "./dist/jsx-dev-runtime.mjs"
    },
    "./h": {
      "types": "./dist/h.d.mts",
      "import": "./dist/h.mjs",
      "require": "./dist/h.mjs"
    },
    "./reactivity": {
      "types": "./dist/reactivity.d.mts",
      "import": "./dist/reactivity.mjs",
      "require": "./dist/reactivity.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "jsx-runtime": [
        "./dist/jsx-runtime.d.ts"
      ],
      "jsx-dev-runtime": [
        "./dist/jsx-dev-runtime.d.ts"
      ],
      "h": [
        "./dist/h.d.ts"
      ],
      "reactivity": [
        "./dist/reactivity.d.ts"
      ],
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint-staged": "lint-staged",
    "tag": "bumpp",
    "release": "bumpp && npm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.1.0",
    "@antfu/ni": "^24.3.0",
    "@playwright/test": "1.51.0",
    "@vitest/coverage-v8": "^0.34.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.3.1",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.11.1",
    "tsdown": "0.20.0-beta.4",
    "typescript": "~5.9.3",
    "vite": "^4.4.5",
    "vite-bundle-analyzer": "^1.3.2",
    "vitest": "^0.34.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
